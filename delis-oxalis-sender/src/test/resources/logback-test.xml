<?xml version="1.0" encoding="UTF-8"?>

<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d %p [%c] %m %n</pattern>
		</encoder>
	</appender>

	<logger name="dk.erst.delis.oxalis.sender" level="debug" />

	<root level="info">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
